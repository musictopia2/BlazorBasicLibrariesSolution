@using cc2 = BasicBlazorLibrary.Components.CssManagement.CssSectionHelpers
<style>
    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }
</style>
<HeadContent>
    <SectionOutlet SectionName="@cc2.PreloadCssSection" />
    @if (UseBlazorAssetMappingFormat == false)
    {
        <link href="_content/BasicBlazorLibrary/custom-bootstrap.css" rel="stylesheet" />
    }
    else
    {
        <link href="@Assets["_content/BasicBlazorLibrary/custom-bootstrap.css"]" rel="stylesheet" />
    }
    <SectionOutlet SectionName="@cc2.PreBaseCssSection" />
    <SectionOutlet SectionName="@cc2.BaseCssSection" />
    <SectionOutlet SectionName="@cc2.ComponentCssSection" />
    <SectionOutlet SectionName="@cc2.PostComponentCssSection" />
    @if (string.IsNullOrWhiteSpace(AppStyleName) == false)
    {
        @if (UseBlazorAssetMappingFormat == false)
        {
            <link rel="stylesheet" href="@ScopedName" />
        }
        else
        {
            <link rel="stylesheet" href="@Assets[@ScopedName]" />
        }
    }
    <SectionOutlet SectionName="@cc2.PostScopedCssSection" />
</HeadContent>