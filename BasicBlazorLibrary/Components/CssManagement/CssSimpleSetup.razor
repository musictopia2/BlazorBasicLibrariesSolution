<FullCssSetup AppStyleName="@AppStyleName" UseBlazorAssetMappingFormat=UseBlazorAssetMappingFormat />
<BaseCssLoader>
    @foreach (var item in CssFiles)
    {
        @if (UseBlazorAssetMappingFormat == false)
        {
            <link rel="stylesheet" href=@item />
        }
        else
        {
            <link rel="stylesheet" href=@Assets[@item] />
        }
    }
</BaseCssLoader>
@ChildContent