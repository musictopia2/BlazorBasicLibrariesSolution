@using System.Drawing 
<SvgBoard BoardClicked="SaveClicked"
          TargetHeight="@TargetHeight"
          BoardSize="new SizeF(500, 500)"
          TargetWidth="@TargetWidth">
    <svg>
        <polygon points="0,0 400,0 500,100 500,500 0,500" style="background-color: @BackgroundColor" />
        <rect x="50" y="50" width="300" height="100" fill="@FillColor" />
        <rect x="175" y="300" width="150" height="150" rx="75" ry="75" fill="@FillColor" />
    </svg>
</SvgBoard>